{
  "version": 2,
  "builds": [
    {
      "src": "bot.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "public/**",
          ".wwebjs_auth/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bot.js"
    }
  ]
}