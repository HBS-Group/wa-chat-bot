{
    "version": 2,
    "builds": [
      {
        "src": "bot.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/styles.css",
        "dest": "/public/styles.css"
      },
      {
        "src": "/script.js",
        "dest": "/public/script.js"
      },
      {
        "src": "/(api|webhook)(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }