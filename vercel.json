{
    "version": 2,
    "builds": [
      {
        "src": "bot.js",
        "use": "@vercel/node",
        "config": {
        "includeFiles": ["node_modules/puppeteer/.local-chromium/**"]
      }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/styles.css",
        "dest": "/public/styles.css"
      },
      {
        "src": "/script.js",
        "dest": "/public/script.js"
      },
      {
        "src": "/(.*)",
        "dest": "bot.js"
      }
    ]
  }